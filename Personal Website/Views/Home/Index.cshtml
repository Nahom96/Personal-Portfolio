@{
    ViewData["Title"] = "Home";
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
    <link rel="stylesheet" href="~/css/homeStyle.css" asp-append-version="true" />
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.3/font/bootstrap-icons.min.css" />
    <link rel="stylesheet" href="/css/about.css" asp-append-version="true"/>
    <link rel="stylesheet" href="/css/skill.css" asp-append-version="true" />

</head>
<body id="page-top">
    <nav class="navbar navbar-expand-sm fixed-top navbar-dark box-shadow mb-3" id="mainNav">
        <div class="container-fluid">
            <a class="name" href="#">Nahom Araya</a>
            <button class="navbar-toggler"
                    type="button"
                    data-bs-toggle="collapse"
                    data-bs-target=".navbar-collapse"
                    aria-controls="navbarResponsive"
                    aria-expanded="false"
                    aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="navbar-collapse collapse d-sm-inline-flex justify-content-end">
                <ul class="navbar-nav">
                    <li class="nav-item"><a class="nav-link" href="#aboutSection">About</a></li>
                    <li class="nav-item"><a class="nav-link" href="#skill">Skills</a></li>
                    <li class="nav-item"><a class="nav-link" href="#">Projects</a></li>
                    <li class="nav-item"><a class="nav-link" href="#">Contact</a></li>
                </ul>
            </div>
        </div>
    </nav>
    <header class="masthead">
        <div class="container px-4 px-lg-5 h-100">
            <div class="row gx-4 gx-lg-5 h-100 align-items-center justify-content-center text-center">
                <div class="col-lg-8 align-self-end">
                    <h1 class="text-white font-weight-bold">Nahom Araya</h1>
                    <hr class="divider" />
                </div>
                <div class="col-lg-8 align-self-baseline">
                    <p class="text-white-75 mb-5">Welcome to my portfolio website! Here, you'll find insights into who I am, my skills, 
                        ongoing learning journey, projects, and professional experience. Thank you for visiting, Enjoy! 😊</p>
                    <button class="btn" id="getStartedBtn">Get Started</button>
                </div>
            </div>
        </div>
    </header>

    <section id="aboutSection">
        <div class="container text-center">
            <h1 id="header">About Me</h1>
        </div>
        <div class="about">
            <h3 id="miniHeaderOne">Who Am I?</h3>
            <p>I am a third-year Centennial College student specializing in software engineering technology.
                <br/>
                <br />
                I have 14 months of work experience, and I recently wrapped up an 8-month internship with the 
                Pain Prediction Research team at Healthcare Systems R&A Inc., 
                I helped with research, dataset integration for increased accuracy, and machine learning model training 
                throughout this period.
                <br />
                <br />
                Before that, I worked as a backend intern at Avolta Inc., where I worked on a variety of web development 
                projects utilizing Flask. Together with my education, these experiences have given me proficiency in data 
                analysis, object-oriented programming, and web development.
            </p>
        </div>
    </section>
    <section id="skill">
        <div class="container text-center">
            <h1 id="skill-header">Technical Skillset Overview</h1>
            <div class="skill-underline"></div>

            <!-- Languages Row -->
            <div class="skill-icons-row">
                <div class="skill-item">
                    <img src="https://cdn.jsdelivr.net/gh/devicons/devicon@latest/icons/python/python-original.svg" alt="Python" />
                    <p>Python</p>
                </div>
                <div class="skill-item">
                    <img src="https://cdn.jsdelivr.net/gh/devicons/devicon@latest/icons/csharp/csharp-original.svg" alt="C#" />
                    <p>C#</p>
                </div>
                <div class="skill-item">
                    <img src="https://cdn.jsdelivr.net/gh/devicons/devicon@latest/icons/sqldeveloper/sqldeveloper-plain.svg" alt="SQL Developer" />
                    <p>SQL Developer</p>
                </div>
                <div class="skill-item">
                    <img src="https://cdn.jsdelivr.net/gh/devicons/devicon@latest/icons/java/java-original.svg" alt="Java" />
                    <p>Java</p>
                </div>
            </div>

            <!-- Frameworks and Tools Row (Row 2) -->
            <div class="skill-icons-row">
                <div class="skill-item">
                    <img src="https://cdn.jsdelivr.net/gh/devicons/devicon@latest/icons/javascript/javascript-plain.svg" alt="JavaScript" />
                    <p>JavaScript</p>
                </div>
                <div class="skill-item">
                    <img src="https://cdn.jsdelivr.net/gh/devicons/devicon@latest/icons/dot-net/dot-net-original.svg" alt=".NET" />
                    <p>.NET</p>
                </div>
                <div class="skill-item">
                    <img src="https://cdn.jsdelivr.net/gh/devicons/devicon@latest/icons/git/git-original.svg" alt="Git" />
                    <p>Git</p>
                </div>
                <div class="skill-item">
                    <img src="https://cdn.jsdelivr.net/gh/devicons/devicon@latest/icons/unix/unix-original.svg" alt="Unix" />
                    <p>Unix</p>
                </div>
            </div>
        </div>
        <p></p>
    </section>



    <section id="ProjectsSection">

    </section>

    <section id="ContactSection">

    </section>


    <script src="/js/home.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js"></script>
</body>
</html>